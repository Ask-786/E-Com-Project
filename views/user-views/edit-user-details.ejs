<body>
  <section class="solid_banner_area">
    <div class="container">
      <div class="solid_banner_inner">
        <h3>checkout register</h3>
        <ul>
          <li><a href="#">Home</a></li>
          <li><a href="checkout.html">Checkout Register</a></li>
        </ul>
      </div>
    </div>
  </section>

  <section class="register_area p_100">
    <div class="container">
      <div class="register_inner">
        <div class="row">
          <div class="col-lg-2"></div>
          <div class="col-lg-8">
            <div class="user_details">
              <h2 class="reg_title">Update Details</h2>
              <form class="billing_inner row" action="/userprofile/edit-user-details" method="post">
                <div class="col-lg-12">
                  <div class="form-group">
                    <label for="fullname">Full Name </label>
                    <input type="text" class="form-control" value="<%= user.fullname %>" id="fullname" name="fullname"
                      aria-describedby="fullname" placeholder="" required>
                  </div>
                </div>
                <div class="col-lg-12">
                  <div class="form-group">
                    <label for="city">Username</label>
                    <input type="text" class="form-control" value="<%= user.username %>" id="username" name="username"
                      aria-describedby="username" placeholder="" required>
                  </div>
                </div>
                <div class="col-lg-12">
                  <div class="form-group">
                    <label for="phone">Phone</label>
                    <input type="number" class="form-control" value="<%= user.phone %>" id="phone" name="phone"
                      aria-describedby="phone" placeholder="" required>
                  </div>
                </div>
                <div class="col-lg-12">
                  <div class="form-group">
                    <label for="state">Email </label>
                    <input type="text" class="form-control" value="<%= user.email %>" id="email" name="email"
                      aria-describedby="email" placeholder="" required>
                  </div>
                  <button type="submit" class="add_cart_btn">Submit</button>
                </div>
              </form>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>
</body>